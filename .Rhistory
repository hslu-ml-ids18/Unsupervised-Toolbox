setwd("C:\\Users\\garde\\Documents\\Studium\\Git\\Unsupervised-Toolbox")
raw = read.csv2("data.csv", header = TRUE, stringsAsFactors = F, dec = ".")
data = raw[,3:length(raw)]
rownames(data) = raw[,1]
scaled_data = as.matrix(scale(data))
hc1= hclust(dist(scaled_data))
plot(hc1)
cutree(hc1, k = 7)
cutree(hc1, h = 7)
heatmap(scaled_data, Colv = F, scale = "none")
thc1 = hclust(dist(t(scaled_data)))
plot(thc1)
cutree(thc1, h = 14)
